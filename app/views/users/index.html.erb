<h1>user list</h1>

<div class="row">
    <div class="col-sm-12">
        <table class="table">
            <tr>
                <th>ID</th>
                <th>Name</th>
                <th></th>
            </tr>
            <% @users.each do |user| %>
                <tr>
                    <td><%= user.id %></td>
                    <td><%= user.username %></td>
                    <td></td>
                </tr>
            <% end -%>
        </table>

        <%= will_paginate @users %>
    </div>
</div>